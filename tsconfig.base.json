            "@nextrap/nt-scope": ["nextrap-base/nt-scope/src/index.ts"],
            "@nextrap/nte-burger": ["nextrap-elements/nte-burger/src/index.ts"],
            "@nextrap/nte-card": ["nextrap-elements/nte-card/src/index.ts"],
            "@nextrap/nte-dialog": ["nextrap-elements/nte-dialog/src/index.ts"],
            "@nextrap/nte-element-highlighter": ["nextrap-elements/nte-element-highlighter/src/index.ts"],
            "@nextrap/nte-nav": ["nextrap-elements/nte-nav/src/index.ts"],
