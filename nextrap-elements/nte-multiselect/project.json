{
    "name": "nte-multiselect",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "nextrap-elements/nte-multiselect/src",
    "projectType": "library",
    "tags": ["type:element", "scope:nextrap"],
    "targets": {
        "build": {
            "executor": "@nx/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/nextrap-elements/nte-multiselect",
                "configFile": "nextrap-elements/nte-multiselect/vite.config.ts"
            }
        },
        "serve": {
            "executor": "@nx/vite:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "nte-multiselect:build",
                "configFile": "nextrap-elements/nte-multiselect/vite.config.ts"
            },
            "configurations": {
                "development": {
                    "buildTarget": "nte-multiselect:build:development"
                },
                "production": {
                    "buildTarget": "nte-multiselect:build:production"
                }
            }
        },
        "test": {
            "executor": "@nx/vite:test",
            "inputs": ["default", "^production"],
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "reportsDirectory": "../../coverage/nextrap-elements/nte-multiselect",
                "coverage": true,
                "configFile": "nextrap-elements/nte-multiselect/vite.config.ts"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint"
        }
    }
}
